    <div class="mt-5">
        <!-- <div class="row"> -->
        <table id="target" class="display ml-5" style="width: 95%; border-radius: 20px;">
            <thead>
                <tr>
                    <th scope="col">Data of <%= domainName.replace(/_/g,".") %>
                    </th>
                </tr>
            </thead>
            <tbody style="color: black;">
                <% data.rows.forEach((target)=>{%>
                    <tr>
                        <td>
                            <form method="POST" action='/show-value/' style="display: inline;">
                                <div style="display: inline;">
                                    <input type="text" hidden name="dataOf" value=<%=domainName%>>
                                    <input type="text" hidden name="check" value=<%=target.key %>>
                                    <button type='submit' style="border: none;background: none;">
                                        <%= (target.key).replace(/_/g," ") %>
                                    </button>
                                </div>
                                <div style="display: inline;" class="float-right">
                                    <input type="checkbox" name="delete" value="delete">
                                    <button type='submit' style="border: none;background: none;">
                                        <i class="fa fa-trash"></i>
                                    </button>
                                </div>

                            </form>
                        </td>
                    </tr>
                    <% })%>

            </tbody>
        </table>
        <!-- </div> -->


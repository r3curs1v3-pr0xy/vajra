                $("#shodanDorksModal").modal('show')
            })
            $("#ongoingScan").click(function () {
                $("#ongoingscan").modal('show')
            })
            $('.chart').easyPieChart({
                barColor:'#17d3e6',
                scaleColor:false,
                lineWidth:6
            })
        });
    </script>

